<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/parker/unraid-templates/master/parker/nextpvr-parker.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Tools:</Category>
  <Name>nextpvr-parker</Name>
  <Description>
    [b]Description[/b][br]
    [br][br]
    [b]Configuration[/b][br]
    [b]/config[/b] This is where the application will store its configuration files, database and logs etc.[br][br]
    [b]Documentation[/b][br]
    https://github.com/parker/documentation[br][br]
  </Description>
  <Overview>
    NextPVR
  </Overview>
  <Support></Support>
  <ReadMe></ReadMe>
  <Registry>https://https://hub.docker.com/r/nextpvr/nextpvr_amd64/</Registry>
  <GitHub>https://github.com/parkerpatel/nextpvr-parker</GitHub>
  <Repository>nextpvr/nextpvr_amd64:stable</Repository>
  <Project></Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8866</HostPort>
        <ContainerPort>8866</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>16891</HostPort>
        <ContainerPort>16891</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment></Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/nextpvr/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user</HostDir>
      <ContainerDir>/recordings</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user</HostDir>
      <ContainerDir>/buffer </ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Banner></Banner>
  <WebUI>https://[IP]:[PORT:3052]/</WebUI>
  <Icon></Icon>
  <DonateText></DonateText>
  <DonateLink></DonateLink>
  <DonateImg></DonateImg>
  <ExtraParams></ExtraParams>
</Containers>
