<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/parker/unraid-templates/master/parker/powerpanelbusiness.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Tools:</Category>
  <Name>parker-powerpanelbusiness</Name>
  <Description>
    [b]Description[/b][br]
    [br][br]
    [b]Configuration[/b][br]
    [b]/config[/b] This is where the application will store its configuration files, database and logs etc.[br][br]
    [b]Documentation[/b][br]
    https://github.com/parker/documentation[br][br]
  </Description>
  <Overview>
    PowerPanel Business
  </Overview>
  <Support></Support>
  <ReadMe></ReadMe>
  <Registry>https://hub.docker.com/r/nathanvaughn/powerpanel-business/</Registry>
  <GitHub>https://github.com/parkerpatel/powerpanel-business-docker</GitHub>
  <Repository>nathanvaughn/powerpanel-business:local-470</Repository>
  <Project></Project>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3052</HostPort>
        <ContainerPort>3052</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>JAVA_VERSION</Name>
      <Value>latest</Value>
    </Variable>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/usr/local/ppbe/db_local</HostDir>
      <ContainerDir>/usr/local/ppbe/db_local</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Banner></Banner>
  <WebUI>https://[IP]:[PORT:3052]/</WebUI>
  <Icon></Icon>
  <DonateText></DonateText>
  <DonateLink></DonateLink>
  <DonateImg></DonateImg>
  <ExtraParams></ExtraParams>
</Containers>
